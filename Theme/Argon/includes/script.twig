<script>
    const logoutForm = document.getElementById("logout-form")
    const logout = function () {
        if (logoutForm === undefined) return;
        logoutForm.submit()
    }
    window.confirmationText = "{{ trans('admin.confirmation') }}"
    window.CLIENTXCMSCurrency = "{{ config('app.currency') }}"

    function confirmation() {
        return confirm(window.confirmationText)
    }
</script>
